a=[]
n=int(input("enter the number of elements in list:"))
for x in range(0,n):
    element=input("Enter element:")
    a.append(element)
max1=len(a[0]) 
temp=a[0]
for i in a:
    if(len(i)>max1):
        maxi=len(i)
        temp=i
print("the word with the longest lenth is: ",temp,"and the length is:",max1)        
